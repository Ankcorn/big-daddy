{
	"name": "@databases/benchmarks",
	"version": "0.0.1",
	"private": true,
	"type": "module",
	"scripts": {
		"build": "echo 'No build step needed'",
		"dev": "wrangler dev",
		"deploy": "wrangler deploy",
		"check:type": "tsc --noEmit",
		"check:lint": "biome check .",
		"bench": "artillery run artillery.yml",
		"bench:simple": "artillery run artillery-simple.yml",
		"blog:setup": "curl -X POST http://localhost:8787/setup",
		"blog:seed": "curl -X POST http://localhost:8787/seed -H 'Content-Type: application/json' -d '{\"users\":10,\"posts\":50,\"comments\":100,\"likes\":200}'",
		"blog:stats": "curl http://localhost:8787/stats",
		"blog:reset": "curl -X POST http://localhost:8787/reset"
	},
	"dependencies": {
		"big-daddy": "workspace:*"
	},
	"devDependencies": {
		"@cloudflare/workers-types": "catalog:default",
		"@databases/tsconfig": "workspace:*",
		"artillery": "^2.0.0",
		"typescript": "catalog:default",
		"wrangler": "catalog:default"
	}
}
