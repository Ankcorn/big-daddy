{
	"name": "big-daddy",
	"version": "0.0.0",
	"private": true,
	"type": "module",
	"exports": {
		".": "./src/index.ts"
	},
	"scripts": {
		"build": "pnpm run css:build",
		"dev": "concurrently \"pnpm run css:watch\" \"wrangler dev\"",
		"deploy": "pnpm run build && wrangler deploy",
		"css:build": "tailwindcss -i ./src/styles.css -o ./src/assets/styles.css",
		"css:watch": "tailwindcss -i ./src/styles.css -o ./src/assets/styles.css --watch",
		"cf-typegen": "wrangler types",
		"check:type": "tsc --noEmit",
		"check:lint": "biome check .",
		"test": "vitest run",
		"test:ci": "vitest run"
	},
	"dependencies": {
		"@databases/sqlite-ast": "workspace:*",
		"effect": "^3.19.3",
		"hatchlet": "^0.0.3",
		"hono": "^4.10.4"
	},
	"devDependencies": {
		"@cloudflare/vitest-pool-workers": "catalog:default",
		"@cloudflare/workers-types": "catalog:default",
		"@databases/tsconfig": "workspace:*",
		"@tailwindcss/cli": "^4.1.16",
		"autoprefixer": "^10.4.21",
		"concurrently": "^9.1.0",
		"postcss": "^8.5.6",
		"tailwindcss": "^4.1.16",
		"typescript": "catalog:default",
		"vitest": "catalog:default",
		"wrangler": "catalog:default"
	}
}
