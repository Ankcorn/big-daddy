{
	"$schema": "https://turbo.build/schema.json",
	"tasks": {
		"build": {
			"dependsOn": ["^build"],
			"outputs": ["dist/**"]
		},
		"dev": {
			"persistent": true,
			"cache": false
		},
		"check:type": {
			"dependsOn": ["^build"]
		},
		"check:lint": {},
		"test": {
			"dependsOn": ["^build"]
		},
		"test:ci": {
			"dependsOn": ["^build"]
		}
	}
}
